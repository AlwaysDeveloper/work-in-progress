{
    "all": true,
    "check-coverage": true,
    "extension": [".js"],
    "exclude": [
        "lib/**",
        "migrations/**",
        "aws/**",
        "constants/**",
        "config/**",
        "lib/**",
        "utils/**",
        "models/**",
        "test/**",
        "app.js",
        "index.js",
        ".mocharc.js",
        "**/*.{test,spec,e2e}.js"
